class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        dict1 = {}
        n = len(nums)
        
        for i in range(n):
            if nums[i] not in dict1:
                dict1[nums[i]] = i
        
        for i in range(n):
            rem = target - nums[i]

            if rem in dict1 and (i != dict1[rem]):
                return [i, dict1[rem]]
    
